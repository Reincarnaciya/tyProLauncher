<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.*?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="A1" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="800.0"
            fx:controller="tylauncher.Controllers.PlayController">
    <VBox alignment="TOP_CENTER" fillWidth="false" nodeOrientation="LEFT_TO_RIGHT" prefHeight="500.0"
          prefWidth="93.0" style="-fx-background-color: black; -fx-alignment: center; -fx-spacing: 4;">
        <ImageView fx:id="Account_Img" fitWidth="75.0" nodeOrientation="INHERIT" pickOnBounds="true"
                   preserveRatio="true">
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
            <Image url="@../assets/notpick/account.png"/>
        </ImageView>
        <ImageView fx:id="News_Img" fitWidth="75.0" nodeOrientation="INHERIT" pickOnBounds="true"
                   preserveRatio="true">
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
            <Image url="@../assets/notpick/news.png"/>
        </ImageView>
        <ImageView fx:id="Forum_Img" fitWidth="75.0" nodeOrientation="INHERIT" pickOnBounds="true"
                   preserveRatio="true">
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
            <Image url="@../assets/notpick/forum.png"/>
        </ImageView>
        <ImageView fx:id="Message_Img" fitWidth="75.0" nodeOrientation="INHERIT" pickOnBounds="true"
                   preserveRatio="true">
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
            <Image url="@../assets/notpick/message.png"/>
        </ImageView>
        <ImageView fx:id="Settings_Img" fitWidth="75.0" nodeOrientation="INHERIT" pickOnBounds="true"
                   preserveRatio="true">
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
            <Image url="@../assets/notpick/settings.png"/>
        </ImageView>
        <ImageView fx:id="Play_Img" fitWidth="75.0" nodeOrientation="INHERIT" pickOnBounds="true"
                   preserveRatio="true">
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
            <Image url="@../assets/picked/play.png"/>
        </ImageView>
    </VBox>
    <Pane layoutX="93.0" prefHeight="500.0" prefWidth="708.0" style="-fx-background-color: #363636;">
        <ProgressBar fx:id="Download_ProgressBar" layoutX="202.0" layoutY="387.0" prefHeight="77.0"
                     prefWidth="243.0" progress="0.0"
                     style="-fx-accent: black; -fx-control-inner-background: gray;"/>
        <Text id="downloadText" fx:id="Progressbar_Text" layoutX="202.0" layoutY="430.0" strokeType="OUTSIDE"
              strokeWidth="0.0" style="-fx-text-origin: center;" styleClass="downloadText" text="Text"
              textAlignment="CENTER" wrappingWidth="243.0"/>
        <Button fx:id="Play_Button" layoutX="202.0" layoutY="387.0" mnemonicParsing="false" prefHeight="77.0"
                prefWidth="243.0" text="Играть">
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
        <ImageView fitHeight="351.0" fitWidth="243.0" layoutX="202.0" layoutY="36.0">
            <effect>
                <DropShadow height="184.9" radius="74.4225" spread="0.08" width="114.79"/>
            </effect>
            <Image url="@../assets/TySci.png"/>
        </ImageView>
    </Pane>
</AnchorPane>
